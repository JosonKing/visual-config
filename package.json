{
  "name": "visual-config",
  "displayName": "Visual Config",
  "description": "Visual configuration",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.48.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:visual-config.helloWorld",
    "onView:moduleTree"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "visual-config.helloWorld",
        "title": "Hello World"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "nodeDependencies",
          "name": "Node Dependencies"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.48.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "eslint": "^7.6.0",
    "glob": "^7.1.6",
    "mocha": "^8.0.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.4.0"
  }
}
